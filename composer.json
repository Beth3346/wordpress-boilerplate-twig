{
    "name": "elr/elr-portfolio",
    "type": "wordpress-theme",
    "license": "MIT",
    "description": "wordpress starter theme using Twig and Timber",
    "homepage": "http://www.elizabeth-rogers.com",
    "authors": [
    {
        "name": "Elizabeth Rogers",
        "email": "beth@elizabeth-rogers.com",
        "homepage": "https://github.com/Beth3346"
    }
    ],
    "keywords": [
        "wordpress",
        "theme",
        "twig",
        "timber"
    ],
    "support": {
    },
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0",
        "timber/timber": "^1.1"
    },
    "autoload": {
        "classmap": [
            "framework/CptBuilder.php",
            "framework/CustomFields.php",
            "framework/ThemeOptions.php",
            "framework/helpers/Admin.php",
            "framework/helpers/Date.php",
            "framework/helpers/Loop.php",
            "framework/helpers/Query.php",
            "framework/helpers/Security.php",
            "framework/helpers/Setup.php",
            "framework/helpers/Taxonomy.php",
            "framework/helpers/Utility.php"
        ],
        "files": [
            "framework/custom-posts/CptSkill.php",
            "framework/custom-posts/CptProject.php",
            "framework/custom-posts/CptTutorial.php",
            "framework/custom-posts/CptExperience.php",
            "framework/custom-posts/CptEducation.php",
            "framework/custom-posts/CptReading.php",
            "framework/custom-posts/CptVideo.php",
            "framework/custom-posts/CptRecommendation.php",
            "framework/shortcodes/elr-shortcodes.php"
        ],
        "psr-4": {
            "Theme\\": "elr-portfolio/"
        }
    }
}
